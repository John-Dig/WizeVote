@{
  Layout = "_Layout";
}

@using Vote.Models

@if (Model.Count == 0)
{
    <div>No users created yet.</div>
} else { 
    <h>Choose a user to login as </h>
    <div>
    @foreach (User user in Model)
    {
        <div>
            <p>@Html.ActionLink($"{user.Name}","Login", new { id = user.UserId })</p>
        </div>
    }
    </div>
}
<div>
@Html.ActionLink("Add a user","Create")
</div>